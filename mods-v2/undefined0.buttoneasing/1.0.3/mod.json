{
	"geode": "2.0.0-beta.23",
	"gd": { "win": "2.204", "android": "2.205" },
	"version": "v1.0.3",
	"id": "undefined0.buttoneasing",
	"name": "ButtonEaseEdit",
	"repository": "https://github.com/undefined06855/ButtonEaseEdit",
	"tags": [ "interface", "customization", "enhancement" ],
	"developer": "undefined0",
	"description": "Buttons moving too boring? Customise the easing how you want!",
	"resources": {
		"files": [ "resources/fire.ogg" ]
	},
	"settings": {
		"test-button": { "type": "custom" },
		"chosen-easing": {
			"name": "Easing ID",
			"type": "int",
			"min": 0,
			"max": 17,
			"default": 8
		},
		"easing-list": { "type": "custom" },
		"easing-rate": {
			"name": "Easing Rate (if applicable)",
			"type": "float",
			"min": 0,
			"max": 4,
			"default": 2,
			"control": { "arrows": false }
		},
		"easing-length": {
			"name": "Easing Length",
			"type": "float",
			"min": 0,
			"max": 5,
			"default": 0.5,
			"control": { "arrows": false }
		},
		"easing-amplitude": {
			"name": "Easing Amplitude",
			"type": "float",
			"min": 0,
			"max": 4,
			"default": 1,
			"control": { "arrows": false }
		},
		"easing-speedMod": {
			"name": "Easing Speed Mod",
			"type": "float",
			"min": 0,
			"max": 4,
			"default": 1,
			"control": { "arrows": false },
			"platforms": [ "windows" ]
		}
	}
}
