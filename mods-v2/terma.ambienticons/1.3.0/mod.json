{
	"geode": "2.0.0-beta.20",
	"gd": "2.204",
	"version": "v1.3.0",
	"id": "terma.ambienticons",
	"name": "Ambient Icons",
	"developer": "Terma",
	"description": "The color of the icons syncs with the color of the level you are playing!",
	"tags": ["ambient", "icons", "color", "icons color", "PlayLayer", "player", "misc"],
	"settings": {
		"change-main-color": {
			"name": "Change main color",
			"description": "Makes main color go <cp>ambient</c>",
			"type": "bool",
			"default": true
		},
		"change-secondary-color": {
			"name": "Change secondary color",
			"description": "Makes secondary color go <cp>ambient</c>",
			"type": "bool",
			"default": false
		},
		"change-main-color-dual": {
			"name": "Change dual main color",
			"description": "Makes dual main color go <cp>ambient</c>",
			"type": "bool",
			"default": true
		},
		"change-secondary-color-dual": {
			"name": "Change dual secondary color",
			"description": "Makes dual secondary color go <cp>ambient</c>",
			"type": "bool",
			"default": false
		},
		"update-time": {
			"name": "Update time (ms)",
			"description": "Less time = more smooth color change but less performance",
			"type": "int",
			"default": 800,
			"min": 0,
			"max": 3000
		},
		"player-flash": {
			"name": "Show Player Flash",
			"description": "Show player flashes in dash orbs, spider orbs, teleports and spider dashes (if enabled, if activated it may cause update bugs)",
			"type": "bool",
			"default": false
		}
	}
}