{
	"geode": "2.0.0",
	"version": "v1.1.0",
	"id": "relative.custom_endscreen_text",
	"name": "Custom Endscreen Text",
	"developer": "relative",
	"description": "",
	"repository": "https://github.com/relativemodder/custom-endscreen-text",
	"settings": {
		"static-text-scale": {
			"name": "Static text scale",
			"description": "Allows you to change endscreen text scale",
			"type": "float",
			"default": 1.0,
			"min": 0.1,
			"max": 2.0,
			"control": {
				"slider": true,
				"input": true
			}
		},
		"strings-file": {
			"name": "Strings file",
			"description": "File with strings, separated by new lines.",
			"type": "file",
			"default": ""
		}
	},
	"gd": {
		"win": "2.204",
		"android": "*"
	}
}