{
	"geode": "1.0.0-beta.17",
	"version": "v0.1.0",
	"id": "chs000.modmenutest",
	"name": "Cobalt Menu",
	"developer": "chs000",
	"description": "Simple Mod Menu",
    "dependencies": [
        {
            "id": "camila314.acbypass",
            "version": ">=v1.0.0",
            "required": true
        }
    ],
    "repository": "https://github.com/Igbalabanov/Cobalt-Menu",
    "settings": {
        "ShowOnStartup": {
            "name": "Show On Startup",
            "description": "Should the mod menu show on startup",
            "type": "bool",
            "default": true
        },
        "isDebugOn": {
            "name": "Enable Debug Mode",
            "description": "Enables a menu for debugging cobalt",
            "type": "bool",
            "default": false
        }
    }
}