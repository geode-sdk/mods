{
	"geode": "2.0.0-beta.21",
	"gd": {
		"win": "2.204",
		"android": "2.205"
	},
	"version": "v1.0.3",
	"id": "capeling.coloured-wave-trail",
	"name": "Customizable Wave Trail",
	"developer": "Capeling",
	"description": "Allows you to easily customize the your wave trail through the mod settings.",
	"settings": {
		"colour": {
			"type": "color",
			"default": [255, 0, 0],
			"name": "Player 1 wave trail colour"
		},
		"colour2": {
			"type": "color",
			"default": [255, 0, 0],
			"name": "Player 2 wave trail colour"
		},
		"colour1Enabled": {
			"type": "bool",
			"default": true,
			"name": "Change player 1 wave trail colour"
		},
		"colour2Enabled": {
			"type": "bool",
			"default": true,
			"name": "Change player 2 wave trail colour"
		},
		"solid": {
			"type": "bool",
			"default": false,
			"name": "Solid wave trail"
		},
		"noWave": {
			"type": "bool",
			"default": false,
			"name": "No wave trail"
		},
		"noDefaultTrail": {
			"type": "bool",
			"default": false,
			"name": "No default trail",
			"description": "Hides the regular player trail while in the <cl>wave</c> gamemode."
		}
	}
}